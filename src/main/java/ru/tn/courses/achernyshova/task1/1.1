public class Summa {
    public static void main(String args[]) {

        int Array[] = {12, 8, 9, 2, 15};
        int K = 3, sum = 0;

        for (int i = 0; i <= 4; i++) {
            if (Array[i] % K == 0) {
                sum += Array[i];
            }
        }
        System.out.print(sum);
    }
}
